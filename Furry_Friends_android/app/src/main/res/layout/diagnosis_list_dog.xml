<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/cornealUlcer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:clickable="true"
        android:focusable="true"
        android:text="각막궤양"
        style="@style/Hyperlink"
         />

    <TextView
        android:id="@+id/blepharitis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        style="@style/Hyperlink"
        android:clickable="true"
        android:focusable="true"
        android:text="안검염"
        />

    <TextView
        android:id="@+id/Cataract"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        style="@style/Hyperlink"
        android:clickable="true"
        android:focusable="true"
        android:text="백내장"
        />

    <TextView
        android:id="@+id/conjunctivitis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        style="@style/Hyperlink"
        android:clickable="true"
        android:focusable="true"
        android:text="결막염"
        />

    <LinearLayout
        android:id="@+id/abnormalList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone"
        tools:visibility="visible">
        <TextView
            android:id="@+id/eyelidTumor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:text="안검종양"
            style="@style/Hyperlink"
            />
        <TextView
            android:id="@+id/nuclearSclerosis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:text="핵경화"
            style="@style/Hyperlink"
            />

        <TextView
            android:id="@+id/entropionOfTheEyelid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:text="안검내반증"
            style="@style/Hyperlink"
            />

        <TextView
            android:id="@+id/vitreousDuctility"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:text="유리체변성"
            style="@style/Hyperlink"
            />

        <TextView
            android:id="@+id/mastopathy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:text="유루증"
            style="@style/Hyperlink"
            />

        <TextView
            android:id="@+id/pigmentedKeratitis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="true"
            android:focusable="true"
            android:text="색소침착성 각막염"
            style="@style/Hyperlink"
            />
    </LinearLayout>


</LinearLayout>